#include<stdio.h>
int main()
{
    int top,x,i;
    int arr[5];
    for(top=0;top<=5;top++)
    {
        if(top==5)
        {
            printf("Stack is full\n");
        }
        else
        {
            printf("Enter the element: ");
            scanf("%d",&x);
            arr[top]=x;
        }
    }
return 0;
}
